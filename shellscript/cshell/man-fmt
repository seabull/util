#!/bin/csh -f
#------------------------------------------------------------------------------

set filter = /usr/tmp/.man-fmt.$$

cat <<-Done- > $filter
#
  {
    l ++ ;

    if ( ( l == 4 ) || ( ( 5 < l ) && ( l < 64 ) ) || ( l == 65 ) )
      print \$0 ;

    if ( l == 68 )
      l = 0 ;
  }
#
-Done-

#------------------------------------------------------------------------------

  awk -f $filter

  rm -f $filter

#------------------------------------------------------------------------------
