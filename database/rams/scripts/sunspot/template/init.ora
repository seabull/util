core_dump_dest=_DB_DUMPBASE_/cdump
timed_statistics=TRUE
user_dump_dest=_DB_DUMPBASE_/udump
 
###########################################
# File Configuration
###########################################
control_files=("_CTL_FILE1_", "_CTL_FILE2_", "_CTL_FILE3_")
 
###########################################
# Instance Identification
###########################################
instance_name=_DB_SID_
 
###########################################
# Job Queues
###########################################
job_queue_processes=10
 
###########################################
# MTS
###########################################
