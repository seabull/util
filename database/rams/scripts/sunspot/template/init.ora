###########################################
# Archive
###########################################
log_archive_dest_1="LOCATION=_ARCHLOG_DIR_"
log_archive_format=_DB_SID__%t_%s.dbf
log_archive_start=TRUE
 
###########################################
# Cache and I/O
###########################################
db_block_size=8192
db_cache_size=738197504
db_file_multiblock_read_count=16
 
###########################################
# Cursors and Library Cache
###########################################
open_cursors=300
 
###########################################
# Database Identification
###########################################
